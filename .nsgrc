{
  "name": "My site",
  "primary": ["stylus"],
  "exclude": ["postinstall", "prepublish"],
  "alwaysOnTop": false,
  "theme": "light",
  "watch": {
    "src/scripts": "RESTART stylus",
    "src/styles": "babel-index"
  },
  "silent": ["fail"],
  "hotkeys": {
    "OSX": {
      "Control+Cmd+Alt+s": "babel-scripts",
      "Control+Cmd+Alt+c": "stylus",
      "Control+Cmd+Alt+k": "KILL stylus",
      "Control+Cmd+Alt+r": "RESTART stylus"
    }
  }
}
