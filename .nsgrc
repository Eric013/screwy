{
	"name": "My site",
	"primary": ["stylus"],
	"exclude": ["babel-postinstall", "postinstall"],
	"alwaysOnTop": false,
  "theme": "light",
  "watch": {
    "src/scripts": "RESTART stylus",
    "src/styles": "babel-index"
  },
  "hotkeys": {
  	"babel-scripts": "Control+Cmd+Alt+s",
  	"stylus": "Control+Cmd+Alt+c",
    "KILL stylus": "Control+Cmd+Alt+k",
    "RESTART stylus": "Control+Cmd+Alt+r"
  }
}
