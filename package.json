{
  "name": "npm-scripts-gui",
  "productName": "NSG",
  "version": "0.0.11",
  "description": "GUI for npm scripts",
  "main": "bin/cli.js",
  "bin": {
    "npm-scripts-gui": "bin/cli.js",
    "nsg": "bin/cli.js"
  },
  "scripts": {
    "stylus": "stylus --watch src/styles/base.styl --use autoprefixer-stylus --out styles",
    "stylus-themes": "stylus --watch src/styles/themes --use autoprefixer-stylus --out styles/themes",
    "babel-scripts": "babel src/scripts --out-dir scripts",
    "babel-cli": "babel src/cli.js > bin/cli.js",
    "babel-index": "babel src/index.js > index.js",
    "babel-postinstall": "babel --watch src/postInstall.js --out-file postInstall.js",
    "postinstall": "node postInstall.js"
  },
  "author": "Sam Eaton",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-stylus": "^0.8.1",
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "stylus": "^0.52.4"
  },
  "dependencies": {
    "electron-prebuilt": "^0.35.4",
    "fangs": "0.0.11",
    "minimist": "^1.2.0",
    "ps-tree": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samueleaton/npm-scripts-gui"
  }
}
